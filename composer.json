{
    "repositories": [
        {
            "type": "path",
            "url": "php-server-sdk-shared-tests"
        }
    ],
    "name": "launchdarkly/server-sdk-dynamodb",
    "description": "LaunchDarkly PHP SDK DynamoDB integration",
    "keywords": [
        "launchdarkly",
        "launchdarkly php"
    ],
    "homepage": "https://github.com/launchdarkly/php-server-sdk-dynamodb",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "LaunchDarkly <sdks@launchdarkly.com>",
            "homepage": "http://launchdarkly.com/"
        }
    ],
    "require": {
        "php": ">=7.3",
        "aws/aws-sdk-php": "^3.86",
        "launchdarkly/server-sdk": "^4"
    },
    "require-dev": {
        "launchdarkly/server-sdk-shared-tests": "dev-main",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "LaunchDarkly\\": "src/LaunchDarkly/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaunchDarkly\\Impl\\Integrations\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
